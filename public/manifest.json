{
    "manifest_version": 2,
    "name": "github extension",
    "version": "1.0.0",
    "description": "github辅助插件",
    "icons": {
        "16": "bai.png",
        "32": "bai.png",
        "48": "bai.png",
        "64": "bai.png",
        "128": "bai.png"
    },
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "bai.png",
        "default_title": "2333",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.bilibili.com/"],
            "js": ["js/content.js"],
            "css": ["content.css"]
        }
    ],
    "permissions": [
        "activeTab",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "homepage_url": "https://www.bilibili.com"
}
